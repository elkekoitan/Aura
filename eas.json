{
  "cli": {
    "version": ">= 12.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "ios": {
        "autoIncrement": "buildNumber",
        "bundleIdentifier": "com.aurafashion.app",
        "buildConfiguration": "Release",
        "resourceClass": "m-medium",
        "image": "latest",
        "env": {
          "EXPO_PUBLIC_APP_ENV": "production",
          "EXPO_PUBLIC_ENABLE_ANALYTICS": "true",
          "EXPO_PUBLIC_ENABLE_VIRTUAL_TRYON": "true"
        }
      },
      "android": {
        "autoIncrement": "versionCode",
        "buildType": "app-bundle",
        "resourceClass": "medium",
        "env": {
          "EXPO_PUBLIC_APP_ENV": "production",
          "EXPO_PUBLIC_ENABLE_ANALYTICS": "true",
          "EXPO_PUBLIC_ENABLE_VIRTUAL_TRYON": "true"
        }
      },
      "channel": "production",
      "distribution": "store"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "your-app-store-connect-app-id",
        "appleTeamId": "your-apple-team-id",
        "sku": "aura-fashion-app-ios",
        "language": "en-US",
        "companyName": "Aura Fashion",
        "appName": "Aura Fashion",
        "copyright": "2024 Aura Fashion. All rights reserved.",
        "primaryCategory": "LIFESTYLE",
        "secondaryCategory": "SHOPPING",
        "contentRights": {
          "containsThirdPartyContent": false,
          "usesIdfa": false
        }
      },
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "production",
        "releaseStatus": "completed",
        "changesNotSentForReview": false
      }
    }
  },
  "updates": {
    "url": "https://u.expo.dev/your-project-id"
  }
}
